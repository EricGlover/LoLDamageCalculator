    "icon2": {
      "value": "",
      "description": "icon2"
    },
    "description2": {
      "value": "After exiting invisibility, his next basic attack within 0.25 seconds deals bonus physical damage. This damage can <img src=\"https://vignette.wikia.nocookie.net/leagueoflegends/images/4/41/Critical_strike_icon.png/revision/latest/scale-to-width-down/20?cb=20191126102550\" \t alt=\"Critical strike icon\"  \tclass=\"\" \t \tdata-image-key=\"Critical_strike_icon.png\" \tdata-image-name=\"Critical strike icon.png\" \t \t width=\"20\"  \t height=\"20\"  \t \t \t \t> critically strike.",
      "description": "description2"
    },
    "leveling2": {
